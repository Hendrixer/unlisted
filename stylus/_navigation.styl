// Left nav size
.full,
.scroll,
.material-sidenav-inner
  height 100%

.material-sidenav-left
  z-index 10000

.nav-pic
  height 1%
  border-radius 100%

.nav-hero
  width 100%
  height 30%
  padding 1%
  background-color primaryDark

.nav-overlay
  z-index 9000
  width 100%
  height 100%
  background-color gray
  position absolute
  transform translate3d(0,0,0)

.nav-bar
  box-shadow 0 8px 17px 0 rgba(0, 0, 0, 0.2)
  background-image none
  color accent

.item
  border-style none
  background-color inherit


h1.title
  color accentDark


